<h1>Books</h1>

<% @books.each do |book| %>

<table>
    <thead>
     <tr>
        <th>Title</th>
        <th>Body</th>
     </tr>
    </thead>

    <tbody>
     <% @books.each do |book| %>
      <tr>
        <td><%= book.title %></td>
        <td><%= book.body %></td>
        <td><a href= "show">Show</a></td>
        <td><a href= "edit">Edit</a></td>
        <td><a href ="destroy">Destroy</a></td>
      </tr>
     <% end %>
    </tbody>
</table>

<% end %>